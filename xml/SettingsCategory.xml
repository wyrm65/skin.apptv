			<height>75</height>
			<font>apptv22</font>
			<reverse>yes</reverse>
		</control>
		<control type="button" id="10">
			<!-- Default category button -->
			<width>480</width>
			<height>75</height>
			<font>apptv22</font>
		</control>
		<control type="sliderex" id="13">
			<!-- Default slider button -->
			<height>75</height>
			<font>apptv22</font>
		</control>
		<control type="label" id="14">
			<!-- Default label -->
			<height>75</height>
			<font>apptv22</font>
		</control>
	</controls>
</window>
