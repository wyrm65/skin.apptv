<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol>3</defaultcontrol>
	<controls>
		<include>background</include>
		<include>fake-overlay</include>
		<control type="label" id="2">
			<!-- Settings screen heading -->
			<left>1419</left>
			<top>38</top>
			<width>465</width>
			<height>48</height>
			<font>apptv30</font>
		</control>
		<control type="label">
			<!-- Settings subscreen heading -->
			<left>1419</left>
			<top>93</top>
			<width>465</width>
			<height>48</height>
			<label>$LOCALIZE[5]</label>
			<font>apptv30</font>
		</control>
		<control type="grouplist" id="3">
			<!-- Subheadings area -->
			<left>1410</left>
			<top>159</top>
			<width>480</width>
			<height>675</height>
			<itemgap>0</itemgap>
			<onleft>5</onleft>
			<onup>noop</onup>
			<ondown>20</ondown>
		</control>
		<control type="button" id="20">
			<!-- Setting level button -->
			<left>1410</left>
			<top>834</top>
			<width>480</width>
			<height>75</height>
			<font>apptv22</font>
			<textcolor>listselected-text</textcolor>
			<label>$LOCALIZE[10037]</label>
			<align>right</align>
			<onleft>5</onleft>
			<onright>5</onright>
			<onup>3</onup>
			<onclick>SettingsLevelChange</onclick>
		</control>
		<control type="label">
			<!-- Settings level heading -->
			<left>1437</left>
			<top>834</top>
			<width>480</width>
			<height>75</height>
			<label>$LOCALIZE[10004]:</label>
			<font>apptv22</font>
		</control>
		<!-- Markers for control area items -->
		<control type="group">
			<control type="image">
				<left>726</left>
				<top>138</top>
				<height>24</height>
				<width>24</width>
				<texture>scroll-up.png</texture>
				<visible>Container(5).HasPrevious</visible>
			</control>
			<control type="image">
				<left>726</left>
				<top>914</top>
				<height>24</height>
				<width>24</width>
				<texture>scroll-down.png</texture>
				<visible>Container(5).HasNext</visible>
			</control>
		</control>
		<control type="grouplist" id="5">
			<!-- Controls area -->
			<left>80</left>
			<top>159</top>
			<width>1328</width>
			<height>750</height>
			<itemgap>0</itemgap>
			<onleft condition="String.IsEqual(Skin.String(show-scrollbar),$LOCALIZE[31877])">noop</onleft>
			<onleft condition="!String.IsEqual(Skin.String(show-scrollbar),$LOCALIZE[31877])">23</onleft>
			<onright>3</onright>
			<pagecontrol>23</pagecontrol>
		</control>
		<control type="scrollbar" id="23">
			<left>20</left>
			<top>159</top>
			<width>18</width>
			<height>750</height>
			<onleft>9000</onleft>
			<onright>5</onright>
			<animation condition="!Control.HasFocus(23) + !Container.Scrolling + !String.IsEqual(Skin.String(show-scrollbar),$LOCALIZE[31875])" effect="fade" start="100" end="0" time="200" reversible="true">Conditional</animation>
			<visible>$EXP[isEnabledScrollbar]</visible>
		</control>
		<control type="textbox" id="6">
			<!-- Settings description area -->
			<left>15</left>
			<top>936</top>
			<width>1890</width>
			<height>105</height>
			<font>apptv20</font>
			<align>center</align>
		</control>
		<control type="button" id="7">
			<!-- Default button -->
			<height>75</height>
			<font>apptv22</font>
			<invalidcolor>invalid-text</invalidcolor>
		</control>
		<control type="radiobutton" id="8">
			<!-- Default radiobutton -->
			<height>75</height>
			<font>apptv22</font>
			<radioposx>1266</radioposx>
		</control>
		<control type="spincontrolex" id="9">
