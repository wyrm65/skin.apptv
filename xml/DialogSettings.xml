<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol always="true">5</defaultcontrol>
	<coordinates>
		<left>0</left>
		<top>0</top>
	</coordinates>
	<controls>
		<include>fadebackground</include>
		<control type="image">
