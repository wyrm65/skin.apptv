			<font>apptv25</font>
		</control>
		<control type="image">
			<left>336</left>
			<top>150</top>
			<width>1242</width>
			<height>6</height>
			<texture>separator.png</texture>
		</control>
		<control type="grouplist" id="5">
			<!-- Default Grouplist -->
			<left>366</left>
			<top>189</top>
			<width>1182</width>
			<height>675</height>
			<pagecontrol>60</pagecontrol>
			<onleft>noop</onleft>
			<onright>60</onright>
